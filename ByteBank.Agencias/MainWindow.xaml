<Window x:Class="ByteBank.Agencias.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ByteBank.Agencias"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="600">
    <Canvas Name="Container">

        <StackPanel Canvas.Top="15" Canvas.Left="290">
            <TextBlock>Número</TextBlock>
            <TextBlock Name="txtNumero"></TextBlock>
            <TextBlock>Nombre</TextBlock>
            <TextBlock Name="txtNombre"></TextBlock>
            <TextBlock>Descripción</TextBlock>
            <TextBlock Name="txtDescripcion"></TextBlock>
            <TextBlock>Dirección</TextBlock>
            <TextBlock Name="txtDireccion"></TextBlock>
            <TextBlock>Teléfono</TextBlock>
            <TextBlock Name="txtTelefono"></TextBlock>
        </StackPanel>
        <Button Name="btnBorrar" Content="Borrar registro" Canvas.Left="434" Canvas.Top="304" Click="Button_Click"/>
        <Button Name="btnEditar" Content="Editar registro" Canvas.Left="350" Canvas.Top="304" />
    </Canvas>

</Window>
